#!/bin/sh
# -*- mode: sh; -*-
# $Id: config_withmost,v 1.7 2019/05/05 23:51:36 cvsuser Exp $
#       auto select the terminal interface (ncurses, tinfo, curse, termcap and termlib)
#       enable most other third-party library support
#

./configure_new \
	--with-8bit		\
	--with-icu		\
	--with-apriconv		\
	--with-iconv		\
	--with-enca		\
	--with-dlmalloc		\
	$*

#end

