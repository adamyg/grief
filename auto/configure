	EXTRA_CPPFLAGS="${EXTRA_CPPFLAGS}$cf_new_extra_cppflags"

fi




		fi
	fi
fi



	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking if the compiler supports -Wno-unused-result" >&5
